{
    "name": "@adobe/lit-mobx",
    "version": "0.0.1",
    "description": "Integrating mobx with lit-element!",
    "license": "Apache-2.0",
    "repository": "https://github.com/adobe/lit-mobx",
    "main": "lit-mobx.js",
    "module": "lit-mobx.js",
    "keywords": [
        "lit-html",
        "lit-element",
        "mobx",
        "web component"
    ],
    "files": [
        "/lib/",
        "/src/",
        "/demo/",
        "/lit-mobx.d.ts",
        "/lit-mobx.d.ts.map",
        "/lit-mobx.js",
        "/lit-mobx.js.map"
    ],
    "scripts": {
        "build": "tsc --build tsconfig.json && tsc --build demo/tsconfig.json",
        "build:watch": "tsc --build tsconfig.json -w & tsc --build demo/tsconfig.json -w",
        "prepublishOnly": "npm run build",
        "demo": "npm run build:watch & es-dev-server"
    },
    "peerDependencies": {
        "mobx": "^5.0.0",
        "lit-element": "^2.0.0"
    },
    "devDependencies": {
        "es-dev-server": "^1.10.4",
        "typescript": "^3.4.1",
        "mobx": "^5.0.0",
        "lit-element": "^2.0.0"
    },
    "typings": "lit-mobx.d.ts",
    "publishConfig": {
        "access": "public"
    }
}
